# Lists
myList = ["Red", "Blue", "Orange"]

print(myList[-1])

myList[1] = "Car"
myList.append("Beach")
myList.pop()
print(myList)


# Dictionary
myDog = {"name" : "Roscoe", "color" : "Blonde"}

print(myDog["name"])
print(len(myDog))



#Tuples
myTuple = ("Red", "Blue", "Orange")

print(len(myTuple))
print(myTuple[0])
print(type(myTuple))
